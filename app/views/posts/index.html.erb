<% if current_user.present? %>
	<div>
		<div>
			<span class="h5">Login As: </span><%= current_user.email %>	
		</div>
		<div>
			<%= button_to 'Sign Out',auth.sign_out_path ,method: :delete,class: "btn btn-danger" %>
		</div>	
	</div>
	
<% else %>
	<div class="sign-in-button">
		<%= link_to "sign in",auth.sign_in_path,class: "btn btn-primary" %>
	</div>
<% end %>
